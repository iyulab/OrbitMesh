{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "OrbitMesh": "Debug"
    }
  },
  "DetailedErrors": true,
  "OrbitMesh": {
    "Security": {
      "AllowAnonymous": true,
      "Enrollment": {
        "AutoApprove": true,
        "AutoApprovePatterns": [ "dev-*", "test-*", "local-*" ]
      }
    }
  }
}
